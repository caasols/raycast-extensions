    {
      "name": "search-docs",
      "title": "Search API Docs",
      "subtitle": "Planning Center (PCO)",
      "description": "Search and open Planning Center (PCO) API documentation pages",
      "mode": "view"
    },
    {
      "name": "search-on-web",
      "title": "Search API Docs on Web",
      "subtitle": "Planning Center (PCO)",
      "description": "Search the Planning Center (PCO) API docs on the web",
      "mode": "no-view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Search query",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
