{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "planning-center-api-docs",
  "title": "Planning Center API Docs",
  "description": "Quickly find the docs you need for the Planning Center API.",
  "icon": "pco-logo-blue.png",
  "author": "thomas.harmond",
